     procedure_id : 1199007173763303164
     type_code : 14
     code : |1+
      // Summary: Fills the table with the content of the file
      // Syntax:
      //DisplayList ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      procedure DisplayList()
      
      // Variable
      sSQL is string = [
      SELECT "BookID","Title", "Publisher", "ISBN" FROM "Book"
      ]
      
      // Initialization
      TableDeleteAll(TABLE_Book)
      
      // Run an SQL query
      if gclBase:SQLExecute(sSQL) then
      	// Retrieve the results of the query
      	while gclBase:SQLNext()
      		TableAdd(TABLE_Book,gclBase:SQLRead(1)+TAB+gclBase:SQLRead(2)+TAB+gclBase:SQLRead(3)+TAB+gclBase:SQLRead(4))
      	end
      else
      	Error(<§$000a§>,<§$0009§>)
      	EndProgram()
      end
